<keymap version="1" name="Default copy" parent="$default">
  <action id="CloseAllEditors">
    <keyboard-shortcut first-keystroke="shift ctrl alt f4" />
  </action>
  <action id="DebugClass">
    <keyboard-shortcut first-keystroke="shift ctrl alt f9" />
  </action>
  <action id="NewClass">
    <keyboard-shortcut first-keystroke="shift ctrl alt comma" />
  </action>
  <action id="NewDir">
    <keyboard-shortcut first-keystroke="shift ctrl alt period" />
  </action>
</keymap>